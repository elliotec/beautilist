<%= form_for list, remote: true do |f| %>
  <%= f.check_box :complete %>
  <%= f.submit 'Update' %>
  <%= f.label :complete, list.name %>
  <%= link_to '', edit_list_path(list), class: "glyphicon glyphicon-pencil" %>
  <%= link_to '', list, method: :delete, data: { confirm: 'Are you sure?' }, class: "glyphicon glyphicon-remove", remote: true %>
<% end %>
